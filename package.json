{
   
  "name": "dentalcrm",
  "version": "1.0.0",
  "scripts": {
    "build:css": "sass --no-source-map --style=compressed --load-path=node_modules dental/static/dental/scss/custom-bootstrap.scss dental/static/dental/css/styles.css",
    "watch:css": "sass --watch --load-path=node_modules dental/static/dental/scss:dental/static/dental/css",
    "build": "npm run build:css"
  },

  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.0",
    "copyfiles": "^2.4.1",
    "sass": "^1.70.0"
  }
}

